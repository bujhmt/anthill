{
    "name": "anthill",
    "version": "0.0.1",
    "description": "Web scraping component-based framework",
    "main": "./dist/index.js",
    "scripts": {
        "test": "npm run test -v",
        "build": "tsc",
        "build:watch": "tsc -w",
        "start": "npm run build && node ./dist/index.js",
        "start:watch": "nodemon ./dist/index.js",
        "start:dev": "concurrently \"build:watch\" \"start:watch\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bujhmt/anthill.git"
    },
    "keywords": [
        "scraping",
        "web",
        "framework",
        "component-based",
        "anthill",
        "puppeteer",
        "crawling"
    ],
    "author": "bujhmt",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/bujhmt/anthill/issues"
    },
    "homepage": "https://github.com/bujhmt/anthill#readme",
    "dependencies": {
        "puppeteer": "^13.0.1",
    },
    "devDependencies": {
        "@bujhmt/dev-js": "^0.0.3",
        "@types/puppeteer": "^5.4.4",
        "@types/reflect-metadata": "^0.1.0",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "concurrently": "^7.0.0",
        "eslint": "^8.6.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.4",
        "nodemon": "^2.0.15",
        "typescript": "^4.5.4"
    },
}
